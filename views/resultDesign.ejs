<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Branch</title>
    <link href="https://unpkg.com/tailwindcss@^1.0/dist/tailwind.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/program.css">
    <link href="/css/navbar.css" rel="stylesheet">
    <script src="/js/js.js"></script>
</head>

<body>
    <nav class="navbar navbar-dark bg-dark fixed-top">
        <div class="container-fluid">
            <button class="navbar-toggler" type="button" onclick="fun3()" data-bs-toggle="offcanvas"
                data-bs-target="#offcanvasDarkNavbar" aria-controls="offcanvasDarkNavbar">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="offcanvas offcanvas-start" id="offcanvasDarkNavbar" aria-labelledby="offcanvasDarkNavbarLabel"
                data-bs-scroll="true" data-bs-backdrop="false">
                <div class="offcanvas-body fixed-start">
                    <ul class="navbar-nav justify-content-end flex-grow-1 pe-3">
                        <li class="nav-item">
                            <span class="title"><b>Existing</b>
                            </span>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active bg-black"
                                href="/university/?u_code=<%= data2.u_code %>">BE/BTECH</a>
                        </li>
                        <hr>
                        <li class="nav-item">
                            <span class="title"><b>New</b>
                            </span>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="?u_code=<%= data2.u_code %>">Add Program</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="?u_code=<%= data2.u_code %>&pro_id=<%= data2.pro_id %>">Add
                                College</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="?u_code=<%= data2.u_code %>&pro_id=<%= data2.pro_id %><%= data2.collage_id %>">Add
                                Branch</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>
    <div class="login-box">
        <div>
            <p style="color:white;">You Are addeding to this <%= data2.u_code %> -> <%= data2.pro_id %> -> <%=
                            data2.collage_id %> -> <%= data2.branch_id%>
            </p>
        </div>
        <h2>Branch Details Add</h2>
        <form action="/university/Design?u_code=<%data2.u_code%>&pro_id=<%= data2.pro_id %>&collage_id=<%= data2.collage_id %>&branch_id=<%= data2.branch_id %>"
            method="post">
            <div class="user-box">
                <input type="text" name="name" required="">
                <label>Name
                </label>
            </div>
            <div class="user-box">
                <input type="text" name="aadharNumber" required="">
                <label>aadharNumber
                </label>
            </div>
            <div class="user-box">
                <input type="text" name="email" required="">
                <label>email
                </label>
            </div>
            <div class="user-box">
                <input type="text" name="seatNumber" required="">
                <label>SeatNumber
                </label>
            </div>
            <div class="user-box">
                <input type="text" name="sem" required="">
                <label>Sem
                </label>
            </div>
            <div class="user-box">
                <input type="text" name="year" required="">
                <label>year
                </label>
            </div>
            <div class="user-box">
                <input type="text" name="resultType" required="">
                <label>resultType
                </label>
            </div>
            <div class="user-box">
                <input type="text" name="cpi" required="">
                <label>cpi
                </label>
            </div>
            <div class="user-box">
                <input type="text" name="spi" required="">
                <label>spi
                </label>
            </div>
            <div class="user-box">
                <input type="text" name="cgpa" required="">
                <label>cgpa
                </label>
            </div>
            <% data2.subjects.forEach((obj)=>{ %>
            <div class="user-box">
                <!-- <input type="text" name="" onfocus="fun(this, '40')" onfocusout="focusOutFun(this)" required=""> -->
                <label></label>
            </div>
            <div class="user-box">
                <input type="text" name="mark" required="">
                <label><%= obj.subject_name%></label>
            </div>
            <%});%>
            <!-- <a href="" style="margin: 0;"> -->
                <span></span>
                <span></span>
                <span></span>
                <span></span>
                <input type="submit" value="submit">
            <!-- </a> -->
        </form>
    </div>

    <script>
        var toggle = 1;
        function fun3() {
            if (toggle == 1) {
                document.getElementById("contents").style.marginLeft = "30%";
                toggle = 0;
            }
            else {
                document.getElementById("contents").style.marginLeft = "5%";
                toggle = 1;
            }

        }
    </script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8"
        crossorigin="anonymous"></script>
</body>

</html>