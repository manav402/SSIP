    <div class="login-box">
        <div>
            <p style="color:white;">You Are addeding to this <%= data.university %> -> <%= data.pro_name %>
            </p>
        </div>
        here
        <h2>Collage Details</h2>
        <form action="?u_code=<%= data.u_code %>&pro_id=<%= data.pro_id %>" onsubmit="addID(this)">
            <div class="user-box" style="display:none;">
                <input type="text" name="collage_id" id="collage_id">
                <label>Collage_id</label>
            </div>
            <div class="user-box" style="display:none;">
                <input type="text" name="u_code" id="u_code">
                <label>Collage_id</label>
            </div>
            <div class="user-box" style="display:none;">
                <input type="text" name="pro_id" id="pro_id">
                <label>Collage_id</label>
            </div>
            <div class="user-box">
                <input type="text" name="" onfocus="fun(this, 'Vishwakarma government engineering collage')"
                    onfocusout="focusOutFun(this)" required="">
                <label>Collage_name</label>
            </div>
            <div class="user-box">
                <input type="text" name="" id="short" onfocus="fun(this, 'VGEC')" onfocusout="focusOutFun(this)" required="">
                <label>Collage_short_name</label>
            </div>
            <div class="user-box">
                <input type="text" name="" onfocus="fun(this, 'c_type')" onfocusout="focusOutFun(this)" required="">
                <label>Collage_type</label>
            </div>
            <div class="user-box">
                <input type="text" name="" onfocus="fun(this, 'c_code')" onfocusout="focusOutFun(this)" required="">
                <label>Collage_code</label>
            </div>
            <div class="user-box">
                <input type="text" name="" onfocus="fun(this, 'Gujarat technological university')"
                    onfocusout="focusOutFun(this)" required="">
                <label>University</label>
            </div>
            <div class="user-box">
                <input type="text" name="" onfocus="fun(this, '500')" onfocusout="focusOutFun(this)" required="">
                <label>No_of_student</label>
            </div>
            <div class="user-box">
                <input type="text" name="" onfocus="fun(this, '10')" onfocusout="focusOutFun(this)" required="">
                <label>No_of_course</label>
            </div>
            <!-- <a href="#" style="margin: 0;"> -->
                <span></span>
                <span></span>
                <span></span>
                <span></span>
                <input type="submit" value="submit">
            </a>
        </form>
    </div>

    <script>
        var toggle = 1;
        function fun2() {
            if (toggle == 1) {
                document.getElementById("contents").style.marginLeft = "30%";
                toggle = 0;
            }
            else {
                document.getElementById("contents").style.marginLeft = "5%";
                toggle = 1;
            }
        }
        function addId(obj) {
                // console.log(obj);
                document.getElementById("u_code").value = "<%= data.u_code %>";
                document.getElementById("pro_id").value = "<%= data.pro_id %>";
                document.getElementById("collage_id").value= "<%=data.pro_id %>_"+document.getElementById("short").value.toUpperCase();
                obj.submit();
            }
    </script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8"
        crossorigin="anonymous"></script>
