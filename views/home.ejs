<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Home-Field</title>
    <link rel="" href="">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous">
    <link href="/css/navbar.css" rel="stylesheet">
    <link rel="stylesheet" href="css/tableCSS.css">
</head>

<body>
    <div class="home-field">
        <div class="container">
            <nav class="navbar navbar-dark fixed-top">
                <div class="container-fluid">
                    <button class="navbar-toggler" type="button" onclick="fun()" data-bs-toggle="offcanvas"
                        data-bs-target="#offcanvasDarkNavbar" aria-controls="offcanvasDarkNavbar">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="offcanvas offcanvas-start" id="offcanvasDarkNavbar"
                        aria-labelledby="offcanvasDarkNavbarLabel" data-bs-scroll="true" data-bs-backdrop="false">
                        <div class="offcanvas-body fixed-start">
                            <ul class="navbar-nav justify-content-end flex-grow-1 pe-3">
                                <li class="nav-item">
                                    <span class="rounded-6"><a class="nav-link active bg-black " aria-current="page"
                                            href="/"><img src="../images/homeactive.png">HOME</a>
                                    </span>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/search"><img src="../images/search.png">Search</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/profile"><img src="../images/profile.png">My Profile</a>
                                </li><br>
                                <hr>
                                <li class="nav-item">
                                    <a class="nav-link" href="/logout"><img src="../images/logout.png">LogOut</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </nav>
            <div id="contents">
                <h1>WELCOME <%= name %>
                </h1>
                <h2>Your Results</h2>
                <ul>
                    <a href="/get-result?type=ssc">
                        <li>SSC Result</li>
                    </a>
                    <a href="/get-result?type=hsc">
                        <li>HSC Result</li>
                    </a>
                    <a href="/get-result?type=college">
                        <li>College Result</li>
                    </a>
                    <a href="/get-result?type=diploma">
                        <li>Diploma Result</li>
                    </a>
                </ul>
            </div>
        </div>
    </div>
    <section id="resultOfStuent" style="margin-bottom: 5rem;">
        <div class="container w-100">
            <div class="row">
                <div class="col-md-offset-1 col-md-10" style="width: 100%;">
                    <div class="panel">
                        <div class="panel-heading">
                            <div class="row">
                                <div class="col col-sm-3 col-xs-
                                12">
                                    <h4 class="title">Result of <span>Student</span></h4>
                                </div>

                            </div>
                        </div>
                        <div class="panel-body table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Subject Name</th>
                                        <th>Age</th>
                                        <th>Job Title</th>
                                        <th>City</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>1</td>
                                        <td>Vincent Williamson</td>
                                        <td>31</td>
                                        <td>iOS Developer</td>
                                        <td>Sinaai-Waas</td>
                                        <td>
                                            <ul class="action-list">
                                                <li><a href="#" data-tip="edit"><i class="fa fa-edit"></i></a></li>
                                                <li><a href="#" data-tip="delete"><i class="fa fa-trash"></i></a></li>
                                            </ul>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>2</td>
                                        <td>Taylor Reyes</td>
                                        <td>22</td>
                                        <td>UI/UX Developer</td>
                                        <td>Baileux</td>
                                        <td>
                                            <ul class="action-list">
                                                <li><a href="#" data-tip="edit"><i class="fa fa-edit"></i></a></li>
                                                <li><a href="#" data-tip="delete"><i class="fa fa-trash"></i></a></li>
                                            </ul>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>3</td>
                                        <td>Justin Block</td>
                                        <td>26</td>
                                        <td>Frontend Developer</td>
                                        <td>Overland Park</td>
                                        <td>
                                            <ul class="action-list">
                                                <li><a href="#" data-tip="edit"><i class="fa fa-edit"></i></a></li>
                                                <li><a href="#" data-tip="delete"><i class="fa fa-trash"></i></a></li>
                                            </ul>
                                        </td>
                                    </tr>

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <script>
        var toggle = 1;
        function fun() {
            if (toggle == 1) {
                document.getElementById("contents").style.marginLeft = "25%";
                toggle = 0;
            }
            else {
                document.getElementById("contents").style.marginLeft = "0%";
                toggle = 1;
            }
        }
    </script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8"
        crossorigin="anonymous"></script>
</body>

</html>